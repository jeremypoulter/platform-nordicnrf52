{
  "name": "nordicnrf52",
  "title": "Nordic nRF52",
  "description": "The nRF52 Series are built for speed to carry out increasingly complex tasks in the shortest possible time and return to sleep, conserving precious battery power. They have a Cortex-M4F processor and are the most capable Bluetooth Smart SoCs on the market.",
  "url": "https://www.nordicsemi.com/Products/nRF52-Series-SoC",
  "homepage": "http://platformio.org/platforms/nordicnrf52",
  "license": "Apache-2.0",
  "engines": {
    "platformio": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/platformio/platform-nordicnrf52.git"
  },
  "version": "2.5.0",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json"
  ],
  "frameworks": {
    "arduino-adafruit": {
      "package": "framework-arduinonordicnrf5",
      "script": "builder/frameworks/arduino/arduino.py"
    },
    "arduino": {
      "package": "framework-arduinonordicnrf5",
      "script": "builder/frameworks/arduino/arduino.py"
    },
    "mbed": {
      "package": "framework-mbed",
      "script": "builder/frameworks/mbed.py"
    }
  },
  "packages": {
    "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "version": ">=1.60301.0"
    },
    "framework-mbed": {
      "type": "framework",
      "optional": true,
      "version": "~5.51001.1"
    },
    "framework-arduinoadafruitnordicnrf5": {
      "type": "framework",
      "optional": true,
      "version": "https://github.com/adafruit/Adafruit_nRF52_Arduino.git"
    },
    "framework-arduinonordicnrf5": {
      "type": "framework",
      "optional": true,
      "version": "~1.501.0"
    },
    "tool-sreccat": {
      "version": "~1.164.0"
    },
    "tool-openocd": {
      "type": "uploader",
      "optional": true,
      "version": "~1.1000.0"
    },
    "tool-nrfjprog": {
      "type": "uploader",
      "optional": true,
      "version": "~1.90702.0"
    },
    "tool-jlink": {
      "type": "uploader",
      "optional": true,
      "version": "^1.63208.0"
    },
    "tool-nrfutil": {
      "type": "uploader",
      "optional": true,
      "version": "https://github.com/adafruit/Adafruit_nRF52_Arduino"
    }
  }
}
